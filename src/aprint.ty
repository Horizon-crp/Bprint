from threading import Thread
from sys import stdout
from typing import Optional
async def aprint(*values, sep: Optional[str] = ' ', end: Optional[str] = '\n', File = stdout, flush: bool = False) {
  '''
  The async version of tprint (works with threading too)
  '''
  sep = str(sep)
  e: str = ''
  for i in values {
    e += sep + str(i)
  }
  e += end
  if flush is False {
    def write() {
      File.write(e)
    }
  } else {
    def write() {
      File.write(e)
      flush()
    }
  }
  Thread(target=write).start()
  return None
}
